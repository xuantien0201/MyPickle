{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/backend/api/index.js"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "backend/api/index.js": {
            "runtime": "@vercel/node"
        }
    }
}